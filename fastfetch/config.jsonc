        "break",
        {
            "type": "shell",
            "key": "╭─ {#31} ",
            "keyColor": "white"
        },
        {
            "type": "locale",
            "key": "├─ {#31} ",
            "keyColor": "white"
        },
        {
            "type": "uptime",
            "key": "├─ {#31}󰦖 ",
            "keyColor": "white"
        },
        {
            "type": "packages",
            "key": "├─ {#31}󰏖 ",
            "keyColor": "white"
        },
        {
            "type": "terminal",
            "key": "├─ {#31} ",
            "keyColor": "white"
        },
        {
            "type": "command",
            "shell": "/bin/zsh",
            "text": "echo $WM",
            "format": "{1}",
            "key": "├─ {#31} ",
            "keyColor": "white"
        },
        {
            "type": "command",
            "shell": "/bin/zsh",
            "text": "echo $TERM_FONT",
            "format": "{1}",
            "key": "╰─ {#31} ",
            "keyColor": "white"
        },
        "break",
        {
            "type": "custom",
            "format": "   \u001b[1m\u001b[31m󰊠  \u001b[1m\u001b[32m󰊠  \u001b[1m\u001b[33m󰊠  \u001b[1m\u001b[34m󰊠  \u001b[1m\u001b[35m󰊠  \u001b[1m\u001b[36m󰊠  \u001b[1m\u001b[37m󰊠 "
        }
